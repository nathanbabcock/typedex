---
const samples = import.meta.glob<string>(
  '/src/type-tests/**/*.type-test.ts',
  { query: '?raw', import: 'default', eager: true }
)

type SampleId = keyof typeof samples

export interface Props {
  id: SampleId
}

const { id } = Astro.props
const content = samples[id]
---

<pre>{content}</pre>